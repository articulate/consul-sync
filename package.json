  "dependencies": {
    "@articulate/funky": "^1.9.x",
    "@articulate/gimme": "^1.3.x",
    "debug": "^4.1.x",
    "joi": "^14.x.x",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@articulate/spy": "^0.0.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.8.0",
    "mocha": "^4.0.1",
    "nock": "^9.0.22",
    "nyc": "^11.2.1"
  }
}
