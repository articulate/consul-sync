{
  "name": "@articulate/consul-sync",
  "version": "0.2.3",
  "description": "Consul helper to sync KV to env vars",
  "main": "index.js",
  "repository": "git@github.com:articulate/consul-sync.git",
  "author": "articulate",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
